{
  "cli": {
    "version": ">= 3.16.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "staging": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://playgrounds-api.onrender.com"
      },
      "channel": "staging"
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://playgrounds-api.onrender.com"
      },
      "channel": "production"
    },
    "test": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://playgrounds-api.onrender.com"
      },
      "channel": "test"
    }
  },
  "submit": {
    "production": {}
  }
}
{
  "expo": {
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Playgrounds Near to access your photo",
          "cameraPermission": "Allow Playgrounds Near to access your camera"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Playgrounds Near to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Playgrounds to use your location."
        }
      ]
    ],
    "name": "Playgrounds Near",
    "slug": "playgrounds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#DAECB5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mikelcabezas.playgrounds"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#DAECB5"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.mikelcabezas.playgrounds"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dac7731f-1ade-4bc2-ab77-0a191afd8cd3"
      }
    },
    "owner": "mikelcabezas",
    "scheme": "plygrnd",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/dac7731f-1ade-4bc2-ab77-0a191afd8cd3"
    }
  },
  "build": {
    "preview": {
      "channel": "preview",
      "distribution": "internal"
      // ...
    },
    "production": {
      "channel": "production"
      // ...
    }
  }
}